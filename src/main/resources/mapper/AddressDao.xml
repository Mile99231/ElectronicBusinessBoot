<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zretc.dao.AddressDao">
    <select id="select" resultType="Address">
    select * from addrss where user= #{user}
    </select>
    <insert id="add">
        INSERT INTO `addrss` (name,sex,phone,addrss,user) VALUES (#{name},#{sex},#{phone},#{addrss},#{user})
    </insert>

</mapper>